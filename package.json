{
  "name": "@estrattonbailey/root",
  "version": "0.0.1",
  "description": "The Clare site: React & Graphql",
  "main": "dist/server.js",
  "scripts": {
    "build:server": "webpack --config webpack.server.config.js -p",
    "watch:server": "webpack --config webpack.server.config.js -w",
    "build:app": "webpack -p",
    "watch:app": "webpack -w",
    "build:css": "postcss app/styles/main.css -o public/main.css",
    "watch:css": "postcss -w app/styles/main.css -o public/main.css",
    "build": "NODE_ENV=production npm run build:server && npm run build:app && npm run build:css",
    "watch": "NODE_ENV=development concurrently 'npm run watch:server' 'npm run watch:app' 'npm run watch:css' 'nodemon dist/server.js'",
    "start": "NODE_ENV=production node dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/estrattonbailey/root.git"
  },
  "author": "estrattonbailey",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/estrattonbailey/root/issues"
  },
  "homepage": "https://github.com/estrattonbailey/root#readme",
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.1",
    "cssnano": "^3.10.0",
    "nodemon": "^1.14.11",
    "postcss": "^6.0.17",
    "postcss-calc": "^6.0.1",
    "postcss-cli": "^5.0.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-discard-comments": "^2.0.4",
    "postcss-import": "^11.0.0",
    "postcss-nested": "^3.0.0",
    "standard-loader": "^6.0.1",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "@foil/react": "0.0.2",
    "@picostate/react": "0.0.4",
    "apollo-fetch": "^0.7.0",
    "body-parser": "^1.18.2",
    "classnames": "^2.2.5",
    "compression": "^1.7.1",
    "connect": "^3.6.5",
    "contentful": "^5.1.0",
    "cookie-session": "^2.0.0-beta.3",
    "cross-fetch": "^1.1.1",
    "cxs": "^6.1.0",
    "date-fns": "^1.29.0",
    "foil": "^0.3.0",
    "graphql": "^0.12.3",
    "graphql-tag": "^2.6.1",
    "helmet": "^3.10.0",
    "highlight.js": "^9.12.0",
    "lodash": "^4.17.4",
    "marked": "^0.3.12",
    "ms": "^2.1.1",
    "now-env": "^3.0.3",
    "picostate": "0.0.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "router": "^1.3.2",
    "serve-static": "^1.13.1",
    "unfetch": "^3.0.0"
  }
}
